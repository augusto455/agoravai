<h1>Listing mesas</h1>

<table>
  <thead>
    <tr>
      <th>C mesa</th>
      <th>N user</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @mesas.each do |mesa| %>
      <tr>
        <td><%= mesa.c_mesa %></td>
        <td><%= mesa.n_user %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>

